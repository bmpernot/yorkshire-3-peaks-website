{
  "name": "backend",
  "description": "backend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.699.0",
    "@aws-sdk/client-dynamodb": "^3.398.0",
    "@aws-sdk/lib-dynamodb": "^3.398.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "aws-sdk-client-mock": "^2.0.0",
    "aws-sdk-client-mock-jest": "^2.2.0",
    "jest": "^29.2.1"
  },
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "local": "./scripts/run-local.sh",
    "local-clean-up": "./scripts/run-local-clean-up.sh",
    "init-db-tables": "node ./scripts/configure-local-db-tables.mjs"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "**/__tests__/**/*.mjs?(x)",
      "**/?(*.)+(spec|test).mjs?(x)"
    ],
    "moduleFileExtensions": [
      "mjs",
      "js"
    ],
    "testPathIgnorePatterns": [
      "__tests__/utils"
    ]
  }
}
